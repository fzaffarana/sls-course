{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "checkJs": false,
    "allowJs": false,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "noImplicitUseStrict": false,
    "preserveSymlinks": true,
    "sourceMap": true,
    "incremental": true,
  },
  "exclude": [
    "./node_modules/",
    "./dist/"
  ]
}
