{
  "scripts": {
    "build": "yarn run build:init && yarn run build:js && yarn run build:install",
    "build:init": "yarn run lint && rm -rf dist && mkdir dist",
    "build:js": "yarn run tsc -p tsconfig.json",
    "build:install": "cp package.json dist/ && cd dist && rm -rf tests && yarn install --production",
    "network:on": "sudo ifconfig lo0 alias 172.16.123.1",
    "network:off": "sudo ifconfig lo0 -alias 172.16.123.1",
    "docker": "docker-compose run api sh",
    "lint": "yarn run eslint . --ext .ts",
    "lint:fix": "yarn run eslint . --fix --ext .ts",
    "format:fix": "yarn run prettier --write \"./**/*.{ts,js,json,md}\"",
    "format:check": "yarn run prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "test": "mocha -r ts-node/register src/tests/**/*.test.ts"
  },
  "dependencies": {
    "@middy/core": "^1.1.0",
    "@middy/http-error-handler": "^1.1.0",
    "@middy/http-event-normalizer": "^1.2.0",
    "@middy/http-json-body-parser": "^1.1.0",
    "@middy/validator": "^1.4.0",
    "aws-sdk": "^2.639.0",
    "http-errors": "^1.8.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@types/uuid": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "serverless-pseudo-parameters": "^2.5.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "license": "MIT"
}
